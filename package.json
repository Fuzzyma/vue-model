    "@vuedx/typescript-plugin-vue": "^0.4.1",
    "bcrypt": "^5.0.0",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
